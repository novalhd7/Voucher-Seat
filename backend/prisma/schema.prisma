generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Voucher {
  id            Int    @id @default(autoincrement())
  crew_name     String
  crew_id       String
  flight_number String
  flight_date   String
  aircraft_type String
  seat1         String
  seat2         String
  seat3         String

  @@unique([flight_number, flight_date])
}
